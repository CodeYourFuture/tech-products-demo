{
	"name": "@codeyourfuture/tech-products-demo",
	"version": "0.0.1",
	"description": " Example app for tech products interview experiment",
	"private": true,
	"scripts": {
		"build": "npm --workspace server run build && npm --workspace client run build",
		"postbuild": "buildinfo --output dist/static/build-info.txt",
		"dev": "concurrently --kill-others --names \"client,server\" \"npm --workspace client run dev\" \"npm --workspace server run dev\"",
		"e2e": "concurrently --kill-others --success first --names \"app,e2e\" \"npm:serve\" \"npm --workspace e2e run e2e\"",
		"lint": "npm run lint:eslint && npm run lint:prettier -- --check",
		"lint:eslint": "eslint .",
		"lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
		"lint:prettier": "prettier .",
		"migration": "node-pg-migrate --config-file migration-config.json",
		"prepare": "husky install || echo 'No Husky üê∂'",
		"preserve": "npm run build",
		"serve": "npm start",
		"ship": "npm run lint && npm run test && npm run e2e",
		"prestart": "node bin/run-migrations.js",
		"start": "node dist/server.js",
		"test": "npm --workspaces --if-present run test",
		"test:cover": "npm run test -- -- --coverage"
	},
	"lint-staged": {
		"*.{js,jsx}": "eslint --fix",
		"*": "prettier --ignore-unknown --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeYourFuture/tech-products-demo.git"
	},
	"author": "Jonathan Sharpe <mail@jonrshar.pe>",
	"contributors": [],
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/CodeYourFuture/tech-products-demo/issues"
	},
	"homepage": "https://github.com/CodeYourFuture/tech-products-demo#readme",
	"devDependencies": {
		"@babel/cli": "^7.22.9",
		"@babel/core": "^7.22.9",
		"@babel/eslint-parser": "^7.22.9",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-runtime": "^7.22.9",
		"@babel/preset-env": "^7.22.9",
		"@codeyourfuture/eslint-config-standard": "^4.0.3",
		"@textbook/build-info": "^0.5.3",
		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.3",
		"husky": "^8.0.3",
		"jest": "^29.6.1",
		"lint-staged": "^13.2.3",
		"msw": "^1.2.3",
		"prettier": "^3.0.0",
		"wait-on": "^7.0.1"
	},
	"engines": {
		"node": "^18",
		"npm": "^9"
	},
	"workspaces": [
		"client",
		"e2e",
		"server"
	]
}
